<!-- %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% Content Start %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% --> 
        <div class="content">
            
            <div class="container-fluid">
                <!-- Información general --> 
                <div class="card">

                    
                        <button class="btn btn-warning col-md-12" data-toggle="collapse" data-target="#demo"><i <i class="pe-7s-search"></i> Buscar</button>
                        <div id="demo" class="collapse">
                            <div class="header">
                            <h4 class="title text-center">Buscador</h4>
                            </div>
                                <div class="row">
                                    <div class="col-md-12 text-center">
                                        <label>Categrías de búsqueda</label>
                                    </div>
                                    <form action="/search" method="POST">
                                    <div class="row col-md-offset-1">
                                        <div class="row">
                                
                                            <div class="col-md-4 col-md-offset-1">
                                                <div class="form-group">
                                                    <input type="text" name="search_id_intent" class="form-control" placeholder="Búsqueda por intent ID"/>
                                                </div>
                                            </div>
                                            <div class="col-md-4 col-md-offset-1">
                                                <div class="form-group">
                                                    <input type="text" name="search_question" class="form-control" placeholder="Búsqueda por pregunta general"/>
                                                </div>
                                            </div>



                                        </div>

                                <div class="row col-md-offset-1">
                                    <div class="col-md-12">
                                    
                                        <div class="row">
                                            <div class="col-md-4 col-md-offset-1">
                                                <div class="form-group">
                                                    <input type="text" name="search_tag" class="form-control" placeholder="Búsqueda por tag"/>
                                                </div>
                                            </div>

                                            <div class="col-md-4">
                                                <div class="form-group">
                                                <input type="hidden" name="search_email" id="search_email" value="algo"/>
                                                <input type="hidden" name="search_token" id="search_token" value="algo"/>
                                                <button class="form-control text-center pull-right" type="submit" value="Buscar">Buscar</button>
                                                </form>
                                                </div>
                                            </div>
                                        </div>                                    
                                </div>
                            </div>
                        
                        <!-- insertar categorías de búsqueda --> 
                    </div>
                        </div>

                    



                </div>
