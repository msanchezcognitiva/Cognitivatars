<!-- %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% Content Start %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% --> 
        <div class="content">
            
            <div class="container-fluid">
                <!-- Información general --> 
                <div class="card">
                    <div class="header text-center">
                        <h4 class="title"><strong>Editor</strong></h4>
                    </div>

                </div>
            </div>



            <div class="container-fluid">
                <!-- Información general --> 
                <form action='/updateintent' method='POST' id="myForm">
                
                    <div id="Intents_Collection" ></div>
    

            </div>
                
                    <!-- END: Entity & intent content box -->
                    <!-- Chat View -->
                <div class="card">
                    <div class="header">
                        <h4 class="title text-center"><strong>Respuestas</strong></h4>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="content table-responsive table-full-width">
                                <table class="table table-hover table-striped" id="Answer_table">
                                    <thead>
                                        <th class="text-center"><strong>Respuesta</strong></th>
                                        <th class="text-center"><strong>Ironía</strong></th>
                                        <th class="text-center"><strong>Formalidad</strong></th>
                                        <th class="text-center"><strong>Humor</strong></th>
                                        <th class="text-center"><strong>Acciones</strong></th>
                                    </thead>
                                    <tbody>
                                    <tr>
                                        <td colspan="3">
                                            <div class="row">
                                                <div class="col-md-10">
                                                    <input type="text" class="form-control" id="new_Answer" name="Answer" placeholder="Insertar respuesta" value="" >
                                                </div>
                                                <div class="col-md-2">
                                                    <a onclick="add_rowAnswer();">
                                                        <i class="pe-7s-plus"></i>
                                                    </a>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-4 text-center">
                                                    <label>Ironía</label>
                                                        <div id="slider">
                                                            <input class="bar" type="range" id="rangeIrony" value="50" name="Irony" onchange="rangeIronyvalue.value=value"/>
                                                            <span class="highlight"></span>
                                                            <output id="rangeIronyvalue">50</output>
                                                        </div>
                                                </div>
                                                <div class="col-md-4 text-center">
                                                    <label>Formalidad</label>
                                                        <div id="slider">
                                                            <input class="bar" type="range" id="rangeFormal" value="50" name="Formal" onchange="rangeFormalvalue.value=value"/>
                                                            <span class="highlight"></span>
                                                            <output id="rangeFormalvalue">50</output>
                                                        </div>
                                                </div>
                                                <div class="col-md-4 text-center">
                                                    <label>Humor</label>
                                                        <div id="slider">
                                                            <input class="bar" type="range" id="rangeHumor" value="50" name="Humor" onchange="rangeHumorvalue.value=value"/>
                                                            <span class="highlight"></span>
                                                            <output id="rangeHumorvalue">50</output>
                                                        </div>
                                                </div>
                                            </div>
                                        </td>
                                    </tr>
                                    </tbody>
                                </table>

                            </div>
                    
                        </div>
                    </div>

                    <!-- END:Chat View -->
                </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="col-md-4 pull-right">
                                <input type="hidden" name="form_output" id="output_hidden" value="algo"/>
                                <input type="hidden" name="token" id="token" value="algo"/>
                                <input type="hidden" name="email" id="email" value="algo"/>
                                <input type="hidden" name="Intent_ID" id="Intent_ID" value="algo"/>
                                <button class="form-control text-center btn-info" type="submit" value="Guardar" onclick="get_all_update(this.form)">Guardar y Volver</button>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            </div>
        </div>

<!-- %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% Content END %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% -->
            <footer class="footer">
                <div class="container-fluid">
                    <p class="copyright pull-right">
                        Tars Cognitiva Chile
                        <div id="UserIntents" style="display: none;">{{Document}}</div>
                    </p>
                </div>
            </footer>
        </div>
    </div>

       



